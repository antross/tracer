{
    "author": "Tony Ross",
    "manifest_version": 2,
    "name": "API Tracer",
    "version": "0.1",
    "description": "Traces native API calls from a page",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content-script.js"],
            "run_at": "document_start"
        }
    ],
    "web_accessible_resources": [
        "main.js"
    ]
}
